<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

  <ResourceDictionary.MergedDictionaries>
    <ResourceDictionary Source="/Microsoft.DataTransfer.WpfHost.Basics;component/CommonStyles.xaml" />
  </ResourceDictionary.MergedDictionaries>

  <!-- String resources -->
  
  <system:String x:Key="FilesSummaryKey">Files:</system:String>
  <system:String x:Key="OpenFilesFilterKey">JSON Documents|*.json|All Files|*.*</system:String>
  <system:String x:Key="OpenFilesDefaultExtensionKey">*.json</system:String>

  <system:String x:Key="FileNameKey">File</system:String>
  <system:String x:Key="FileNameSummaryKey">File:</system:String>
  <system:String x:Key="SaveFileFilterKey">JSON Documents|*.json</system:String>
  <system:String x:Key="SaveFileDefaultExtensionKey">*.json</system:String>
  
  <system:String x:Key="PrettifyKey">Prettify JSON</system:String>
  <system:String x:Key="PrettifySummaryKey">Prettify JSON:</system:String>

</ResourceDictionary>